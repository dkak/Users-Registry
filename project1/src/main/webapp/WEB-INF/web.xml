<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
    
  	<welcome-file-list>
    	<welcome-file>/homePage.jsp</welcome-file>
	</welcome-file-list>
    
	<servlet>
		<servlet-name>insertion</servlet-name>
		<servlet-class>com.dkak.InsertServlet</servlet-class>
	</servlet>
  
	<servlet-mapping>
		<servlet-name>insertion</servlet-name>
		<url-pattern>/insert</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>displayUsers</servlet-name>
		<servlet-class>com.dkak.DisplayUsersServlet</servlet-class>
	</servlet>
  
	<servlet-mapping>
		<servlet-name>displayUsers</servlet-name>
		<url-pattern>/displayUsers</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>displayDetailedUser</servlet-name>
		<servlet-class>com.dkak.DisplayDetailedUserServlet</servlet-class>
	</servlet>
  
	<servlet-mapping>
		<servlet-name>displayDetailedUser</servlet-name>
		<url-pattern>/displayDetailedUser</url-pattern>
	</servlet-mapping>
 
</web-app>
